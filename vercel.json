{
  "builds": [
    {
      "src": "html/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "html/register.html",
      "use": "@vercel/static"
    },
    {
      "src": "html/login.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/html/login.html"
    },
    {
      "src": "/login",
      "dest": "/html/login.html"
    },
    {
      "src": "/register",
      "dest": "/html/register.html"
    }
  ]
}
